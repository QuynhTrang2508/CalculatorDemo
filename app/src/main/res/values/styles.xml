<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="button_custom">
        <item name="android:textSize">@dimen/sp_30</item>
        <item name="android:layout_marginLeft">@dimen/dp_8</item>
        <item name="android:layout_marginRight">@dimen/dp_8</item>
        <item name="android:background">@drawable/button_custom</item>
        <item name="layout_constraintHorizontal_weight">1</item>
    </style>
</resources>
